a:76:{s:10:"pluginname";s:8:"Aardvark";s:16:"region-side-post";s:5:"Right";s:15:"region-side-pre";s:4:"Left";s:12:"choosereadme";s:170:"Aardvark is a three column theme for Moodle 2.5+ originally created by Shaun Daubney for <a href="http://www.newbury-college.ac.uk">Newbury College</a> based on Bootstrap";s:11:"configtitle";s:8:"Aardvark";s:12:"basicheading";s:14:"Basic Settings";s:16:"basicheadingdesc";s:38:"Customise basic elements of the theme.";s:16:"frontpageheading";s:9:"Frontpage";s:20:"frontpageheadingdesc";s:71:"Customise frontpage elements of the theme such as user alerts and date.";s:13:"colourheading";s:22:"Colours and Background";s:17:"colourheadingdesc";s:44:"Customise site colours and background image.";s:16:"footeroptheading";s:6:"Footer";s:13:"footeroptdesc";s:47:"Customise basic elements of the theme's footer.";s:18:"socialiconsheading";s:12:"Social Icons";s:22:"socialiconsheadingdesc";s:31:"Customise social network icons.";s:8:"hidemenu";s:25:"Hide Menu when Logged Out";s:12:"hidemenudesc";s:115:"Force users to log in before accessing the menu. If YES then menu options are hidden from users before they log in.";s:9:"backimage";s:16:"Background Image";s:13:"backimagedesc";s:218:"Change the background image in the graphic wrap. It's currently a cloud blue blur, but if that's making you a bit light headed then paste the URL of the image you want to use in here or clear it for a blank background.";s:12:"backposition";s:25:"Background Image Position";s:16:"backpositiondesc";s:48:"Choose between a centred image or a tiled image.";s:19:"backpositioncentred";s:7:"Centred";s:17:"backpositionfixed";s:15:"Centred (Fixed)";s:17:"backpositiontiled";s:14:"Repeat (Tiled)";s:18:"backpositionrepeat";s:18:"Repeat (Horizonal)";s:9:"backcolor";s:17:"Background Colour";s:13:"backcolordesc";s:129:"Change the colour of the background. (This is the area either side of the main content and does not affect the main content area)";s:4:"logo";s:4:"Logo";s:8:"logodesc";s:174:"Change the logo of this theme by entering the URL to a new one (i.e., http://www.somesite/logo.png). Ideally a transparent .png with a maximium height of 38px will work best.";s:14:"menuhovercolor";s:17:"Menu Hover Colour";s:18:"menuhovercolordesc";s:50:"Change the colour of linked items on the menu bar.";s:9:"titledate";s:14:"Frontpage Date";s:13:"titledatedesc";s:43:"Display today&apos;s date on the frontpage.";s:12:"generalalert";s:13:"General Alert";s:16:"generalalertdesc";s:93:"This will display an alert on your frontpage to advise users of a particular event/situation.";s:9:"snowalert";s:10:"Snow Alert";s:13:"snowalertdesc";s:116:"This will display an alert on your frontpage to advise users of closures or information regarding inclement weather.";s:10:"disclaimer";s:10:"Disclaimer";s:14:"disclaimerdesc";s:77:"The content from this textarea will be displayed in the footer of every page.";s:9:"copyright";s:9:"Copyright";s:13:"copyrightdesc";s:30:"The name of your organisation.";s:4:"ceop";s:4:"CEOP";s:8:"ceopdesc";s:22:"Enable CEOP reporting.";s:8:"ceopnone";s:8:"Disabled";s:7:"ceopaus";s:16:"Enable Australia";s:6:"ceopuk";s:21:"Enable United Kingdom";s:5:"email";s:5:"Email";s:8:"emailurl";s:9:"Email URL";s:12:"emailurldesc";s:103:"Enter the URL of your email service. (i.e http://mail.mycollege.ac.uk or http://www.mycollege.edu/mail)";s:7:"website";s:11:"Website URL";s:11:"websitedesc";s:68:"Enter the URL of your main website. (i.e http://www.mycollege.ac.uk)";s:8:"facebook";s:12:"Facebook URL";s:12:"facebookdesc";s:76:"Enter the URL of your Facebook page. (i.e http://www.facebook.com/mycollege)";s:7:"twitter";s:11:"Twitter URL";s:11:"twitterdesc";s:74:"Enter the URL of your Twitter feed. (i.e http://www.twitter.com/mycollege)";s:10:"googleplus";s:11:"Google+ URL";s:14:"googleplusdesc";s:89:"Enter the URL of your Google+ profile. (i.e http://plus.google.com/107817105228930159735)";s:6:"flickr";s:10:"Flickr URL";s:10:"flickrdesc";s:72:"Enter the URL of your Flickr page. (i.e http://www.flickr.com/mycollege)";s:9:"pinterest";s:13:"Pinterest URL";s:13:"pinterestdesc";s:74:"Enter the URL of your Pinterest page. (i.e http://pinterest.com/mycollege)";s:9:"instagram";s:13:"Instagram URL";s:13:"instagramdesc";s:97:"Enter the URL of your Instagram page (via web.stagram). (i.e http://web.stagram.com/n/mycollege/)";s:8:"linkedin";s:12:"LinkedIn URL";s:12:"linkedindesc";s:87:"Enter the URL of your LinkedIn profile. (i.e http://www.linkedin.com/company/mycollege)";s:7:"youtube";s:11:"YouTube URL";s:11:"youtubedesc";s:77:"Enter the URL of your YouTube channel. (i.e http://www.youtube.com/mycollege)";s:9:"wikipedia";s:13:"Wikipedia URL";s:13:"wikipediadesc";s:82:"Enter the URL of your Wikipedia page. (i.e http://en.wikipedia.org/wiki/mycollege)";s:5:"apple";s:19:"Apple App Store URL";s:9:"appledesc";s:107:"Enter the URL of your Apple App Store page. (i.e https://itunes.apple.com/gb/artist/my-college/id123456789)";s:7:"android";s:21:"Google Play Store URL";s:11:"androiddesc";s:115:"Enter the URL of your Google Play Store Store page. (i.e https://play.google.com/store/apps/developer?id=mycollege)";s:9:"customcss";s:10:"Custom CSS";s:13:"customcssdesc";s:119:"Whatever CSS rules you add to this area will be reflected in every page, making for easier customisation of this theme.";s:6:"credit";s:51:"Based on an original theme created by Shaun Daubney";}